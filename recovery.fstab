# Android fstab file.
#<src>                                                  <mnt_point>         <type>    <mnt_flags and options>                                                                   <fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK
/dev/block/mmcblk0p11                                   /efs                ext4      nosuid,nodev,barrier=1                                                                    wait,check
/dev/block/mmcblk0p15                                   /data               ext4      noatime,nosuid,nodev,barrier=1,discard,noauto_da_alloc,journal_async_commit               wait,check,encryptable=footer
/dev/block/mmcblk0p7                                    /boot               emmc      defaults                                                                                  defaults
/dev/block/mmcblk0p18                                   /recovery           emmc      defaults                                                                                  defaults
/dev/block/mmcblk0p17                                   /cache              ext4      nosuid,nodev,barrier=1                                                                    wait,check
/dev/block/mmcblk0p14                                   /system             ext4      ro,barrier=1                                                                              wait
